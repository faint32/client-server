@charset "UTF-8";
@import url("framework.css");

body {
	font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
	background-repeat: repeat-x;
	background-color: #ffffff;
	font-size:12px;
	color: #222222;
}

a {
	text-decoration: none;
	color: #222222;
}
a:hover 
{
    color: #5086a5; 
    text-decoration: underline;
    font-size: 12px;
}
a:visited 
{
    color: #5086a5;
    font-size: 12px;
}
h1 {
	font-size: 16pt;
}

h2 {
	font-size: 14pt;
}

h3 {
	font-size: 12pt;
}

h4 {
	font-size: 10pt;
}

h5 {
	font-size: 9pt;
}

h6 {
	text-transform: uppercase;
	color: #999999;
	font-size: 8pt;
}

code {
	background-color: #ececec;
	padding: 1px 3px;
	color: #666666;
}

code, pre {
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;

	font-family: Monaco, "Courier New", monospace;
	padding: 0px 3px 2px;
}

pre {
	background-color: #f8f8f8;
	border: 1px solid #dddddd;
	white-space: pre-wrap;
	word-wrap: break-word;
	line-height: 1.6em;
	white-space: pre;
	display: block;
	padding: 7px;
}

blockquote {
	border-left: 4px solid #dddddd;
	overflow: hidden;
	display: block;
}

blockquote p {
	margin: 10px 14px !important;
	font-size: 11pt;
}

blockquote small {
	margin: -8px 14px 10px;
	display: block;
	color: #999999;
}

.right {
	float: right;
}

.left {
	float: left;
}

.first {
	margin-left: 14px !important;
}

.last {
	margin-right: 14px !important;
}

[class*="grid"] {
	margin-left: 14px;
	float: left;
}

#page h4 {
	padding-bottom:0px;
}

#page h4.icon {
	background: url("list_wImages.png") 0px -1px no-repeat;
	padding-left:25px;
}


.grid3 { width: 226px; float:right;}
.grid6 { width: 48.6%; }

.grid12 { width: 100%; }
.demo {
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;

	background-color: #eaeaea;
	border: 1px solid #999999;
	margin-bottom: 14px;
	text-align: center;
	cursor: pointer;
	display: block;
	padding: 10px;
}
.row {
	margin-left: -14px; 
}

.row:before, .row:after {
	content: "";
	display: table;
}

.row:after {
	clear: both;
}

.table [class*="grid"], .form [class*="grid"] {
	margin-left: 0px;
	float: none;
}


.form .grid12 { width: 100%; }




.chart {
	height: 200px;
	width: 100%;
}

#page {
	
    margin: 0px auto;
	width:99%;
}


#top {
	background:url(title.jpg) repeat scroll 0 0 transparent;
    padding: 0px 0px;
	border-left:1px #cbdbfd solid;
	border-right:1px #cbdbfd solid;
	margin: 8px 0px 0px 0px;
	padding-bottom:1px;
	overflow: hidden;
	display:block;
}

#top span.divider {
	margin-left: 4px;
}

#top > a {
	float: left;
	font-weight:normal;
}

#top > a, #navigation {
	padding: 12px 14px;
}

#top a {
	font-weight: normal;
	color: #000000;
}

#top > a small {
	font-weight: normal;
	color: #555555;
}

#top + #container {
	border-top: 1px solid #797f90;
}

#navigation {
	list-style: none;
	float: right;
}

#navigation > li {
	margin-left: 8px;
	float: left;
}



#menu {
	list-style: none;
	margin-top: 2px;
	float:right;
}

#menu > li {
	margin-left: 0px;
	float: left;
}

#menu > li > a {
	
	border-left:#F6F6F6 1px solid;
	display: inline-block;
	padding: 8px 8px;
	color:#333;
}

#menu > li > a small {

	padding: 2px 4px 1px;
	font-weight: normal;
	margin-left: 4px;
	font-size: 7pt;
}

#menu > li > a:hover {
	
	color: #578ccd;
}

#menu > li > a:active, #menu > li.active > a {
	
	color: #578ccd;
}

#menu > li > a:hover small {
	
}

#menu > li > a small, #menu > li > a:active small, #menu > li.active > a small {

	color: #333333;
}

#menu > li:not(.active) > a:not(:active):hover small {
	
	color: #ffffff;
}

#menu .caret {
	border-top-color: #ffffff;
}

#menu > li.dropdown:not(.active) > a:not(:active):hover .caret {
	border-top-color: #444444;
}



.block, .panel {
	margin-bottom: 8px;
}


.ie #menu > li:hover > a {
	border: 1px solid #585a60;
	padding: 7px 8px;
}

.ie8 #menu .dropdown > a:hover > .caret {
	border-color: #333333 transparent transparent;
}

.ie #menu > li > a:active, .ie #menu > li.active > a {
	border: 1px solid #40434b;
	padding: 7px 8px;
}

.ie #menu > li > a small, .ie #menu > li > a:active small, .ie #menu > li.active > a small {
	margin: -2px 0px -5px 5px;
	border: 1px solid #585a60;
	display: inline-block;
}

.ie .block h2 {
	background: url("images/gradients.png") repeat-x;
}
.menu li {
	text-shadow: none;
	margin: 3px 0px;
}

.menu:before {
	content: "";
	border-right: 7px solid transparent;
	border-left: 7px solid transparent;
	border-bottom: 7px solid #333333;
	position: absolute;
	left: 5px;
	top: -7px;
}

.menu:after {
	content: "";
	border-right: 6px solid transparent;
	border-left:6px solid transparent;
	border-bottom: 6px solid #ffffff;
	position: absolute;
	left: 6px;
	top: -6px;
}

.menu .divider {
	border-bottom: 1px solid #e5e5e5;
}

.menu .divider.heading {
	color: #222222 !important;
	font-weight: bold;
	padding: 5px 10px;
}

.menu a {
	font-weight: normal !important;
	color: #555555 !important;
	padding: 6px 10px;
	display: block;
}

.menu li > a:hover, .menu li.active > a, .menu li.active > a:hover {
	border-bottom: 1px solid #70747d;
	border-top: 1px solid #70747d;
	background-color: #8d929e;
	color: #ffffff !important;
	padding: 5px 10px;
}
.dropdown {
	display: inline-block;
}

.dropdown.active.js .menu {
	display: block;
}

